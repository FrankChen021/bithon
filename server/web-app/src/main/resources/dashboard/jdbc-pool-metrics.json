{
  "name": "jdbc-pool-metrics",
  "title": "JDBC Connection Pool",
  "folder": "metrics",
  "filter": {
    "selectors": [
      {
        "type": "datasource",
        "name": "jdbc-pool-metrics",
        "fields": [
          "appName",
          "instanceName",
          "server",
          "database"
        ]
      }
    ]
  },
  "charts": [
    {
      "title": "JDBC Connection Pool",
      "width": 4,
      "type": "line",
      "columns": [
        "activeCount",
        "poolingCount",
        "createCount",
        "destroyCount",
        "waitThreadCount"
      ],
      "query": {
        "type": "timeseries",
        "dataSource": "jdbc-pool-metrics",
        "fields": [
          "activeCount",
          "poolingCount",
          "createCount",
          "destroyCount",
          "waitThreadCount"
        ]
      }
    },
    {
      "title": "Transaction",
      "width": 4,
      "type": "line",
      "columns": [
        "executeCount",
        "startTransactionCount",
        "commitCount",
        "rollbackCount"
      ],
      "query": {
        "type": "timeseries",
        "dataSource": "jdbc-pool-metrics",
        "fields": [
          "executeCount",
          "startTransactionCount",
          "commitCount",
          "rollbackCount"
        ]
      }
    }
  ]
}
