{
  "name": "http-incoming-metrics",
  "title": "HTTP Incoming",
  "folder": "metrics",
  "charts": [
    {
      "dataSource": "http-incoming-metrics",
      "title": "QPS",
      "width": 4,
      "type": "line",
      "columns": [
        "qps",
        "okQPS",
        {
          "name": "totalCount",
          "yAxis": 1
        }
      ],
      "query": {
        "type": "timeseries",
        "dataSource": "http-incoming-metrics",
        "fields": [
          "qps",
          "okQPS",
          "totalCount"
        ]
      },
      "details": {
        "columns": [
          "method",
          "uri",
          "statusCode",
          "totalCount",
          "qps"
        ],
        "query": {
          "dataSource": "http-incoming-metrics",
          "fields": [
            "method",
            "uri",
            "statusCode",
            "qps",
            "totalCount"
          ],
          "orderBy": {
            "name": "totalCount",
            "order": "desc"
          }
        },
        "tracing": {
          "mappings": {
            "uri": "url",
            "statusCode": "status"
          },
          "filter": "kind = 'SERVER'"
        }
      }
    },
    {
      "dataSource": "http-incoming-metrics",
      "title": "Response Time",
      "width": 4,
      "type": "line",
      "yAxis": [
        {
          "format": "nanoFormatter"
        }
      ],
      "columns": ["minResponseTime", "avgResponseTime", "maxResponseTime"],
      "query": {
        "dataSource": "http-incoming-metrics",
        "type": "timeseries",
        "fields": [
          "minResponseTime",
          "avgResponseTime",
          "maxResponseTime"
        ]
      },
      "details": {
        "columns": [
          "method",
          "uri",
          "statusCode",
          "totalCount",
          {"name": "minResponseTime", "format": "nanoFormatter"},
          {"name":"avgResponseTime",  "format": "nanoFormatter"},
          {"name": "maxResponseTime", "format": "nanoFormatter"}
        ],
        "query": {
          "dataSource": "http-incoming-metrics",
          "type": "groupBy",
          "fields": [
            "method",
            "uri",
            "statusCode",
            "totalCount",
            "minResponseTime",
            "avgResponseTime",
            "maxResponseTime"
          ],
          "orderBy": {
            "name": "avgResponseTime",
            "order": "desc"
          }
        },
        "tracing": {
          "mappings": {
            "uri": "url",
            "statusCode": "status"
          },
          "filter": "kind = 'SERVER'"
        }
      }
    },
    {
      "dataSource": "http-incoming-metrics",
      "title": "Errors",
      "width": 4,
      "type": "line",
      "columns": [
        "count4xx",
        "count5xx",
        "flowedCount",
        "degradedCount"
      ],
      "query": {
        "dataSource": "http-incoming-metrics",
        "type": "timeseries",
        "fields": [
          "count4xx",
          "count5xx",
          "flowedCount",
          "degradedCount"
        ]
      },
      "details": {
        "dataSource": "http-incoming-metrics",
        "columns": [
          "method",
          "uri",
          "statusCode",
          "totalCount"
        ],
        "query": {
          "type": "groupBy",
          "fields": [
            "method",
            "uri",
            "statusCode",
            "totalCount"
          ],
          "orderBy": {
            "name": "totalCount",
            "order": "desc"
          },
          "filters": [
            {
              "dimension": "statusCode",
              "matcher": {
                "type": "gte",
                "value": "400"
              }
            }
          ]
        },
        "tracing": {
          "mappings": {
            "uri": "url",
            "statusCode": "status"
          },
          "filter": "kind = 'SERVER'"
        }
      }
    },
    {
      "dataSource": "http-incoming-metrics",
      "title": "IO",
      "width": 4,
      "type": "line",
      "yAxis": [
        {
          "format": "byte_rate"
        },
        {
          "format": "binary_byte"
        }
      ],
      "columns": [
        "requestByteRate",
        "responseByteRate",
        {
          "name": "requestBytes",
          "yAxis": 1
        },
        {
          "name": "responseBytes",
          "yAxis": 1
        }
      ],
      "query": {
        "dataSource": "http-incoming-metrics",
        "type": "timeseries",
        "fields": [
          "requestByteRate",
          "responseByteRate",
          "requestBytes",
          "responseBytes"
        ]
      },
      "details": {
        "columns": [
          "method",
          "uri",
          "statusCode",
          "totalCount",
          {"name":"requestByteRate","format": "byte_rate"},
          {"name":"responseByteRate","format": "byte_rate"}
        ],
        "query": {
          "type": "groupBy",
          "fields": [
            "method",
            "uri",
            "statusCode",
            "totalCount",
            "requestByteRate",
            "responseByteRate"
          ],
          "orderBy": {
            "name": "requestByteRate",
            "order": "desc"
          }
        }
      }
    }
  ]
}