{
  "name": "web-server-metrics",
  "title": "Web Server",
  "folder": "metrics",
  "filter": {
    "selectors": [
      {
        "type": "datasource",
        "name": "web-server-metrics",
        "fields": [
          "appName",
          "instanceName"
        ]
      }
    ]
  },
  "charts": [
    {
      "title": "Web Server Connections",
      "width": 4,
      "type": "line",
      "columns": [
        "connectionCount",
        {"name":"maxConnections", "fill": false}
      ],
      "query": {
        "type": "timeseries",
        "dataSource": "web-server-metrics",
        "fields": [
          "connectionCount",
          "maxConnections"
        ]
      }
    },
    {
      "title": "Queued Tasks",
      "width": 4,
      "type": "line",
      "columns": [
        "queueSize"
      ],
      "query": {
        "type": "timeseries",
        "dataSource": "web-server-metrics",
        "fields": [
          "queueSize"
        ]
      }
    },
    {
      "title": "Web Server Threads",
      "width": 4,
      "type": "line",
      "columns": [
        "activeThreads",
        "pooledThreads",
        {"name": "maxThreads", "fill": false}
      ],
      "query": {
        "type": "timeseries",
        "dataSource": "web-server-metrics",
        "fields": [
          "activeThreads",
          "pooledThreads",
          "maxThreads"
        ]
      }
    }
  ]
}
