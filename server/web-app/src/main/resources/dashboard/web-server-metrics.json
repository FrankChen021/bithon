{
  "title": "Web Server",
  "charts": [
    {
      "dataSource": "web-server-metrics",
      "title": "Web Server Connections",
      "width": 4,
      "type": "line",
      "columns": [
        "connectionCount",
        "maxConnections"
      ],
      "query": {
        "type": "timeseries",
        "dataSource": "web-server-metrics",
        "fields": [
          "connectionCount",
          {"name":"maxConnections", "fill": false}
        ]
      }
    },
    {
      "dataSource": "web-server-metrics",
      "title": "Web Server Threads",
      "width": 4,
      "type": "line",
      "columns": [
        "activeThreads",
        {"name": "maxThreads", "fill": false}
      ],
      "query": {
        "type": "timeseries",
        "dataSource": "web-server-metrics",
        "fields": [
          "activeThreads",
          "maxThreads"
        ]
      }
    }
  ]
}
