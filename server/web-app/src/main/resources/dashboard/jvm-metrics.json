{
  "title": "",
  "charts": [
    {
      "dataSource": "jvm-metrics",
      "title": "CPU",
      "width": 2,
      "yAxis": [
        {
          "unit": "percentage"
        }
      ],
      "metrics": [
        {
          "name": "processCpuLoad",
          "unit": "percentage",
          "chartType": "line"
        }
      ]
    },
    {
      "dataSource": "jvm-metrics",
      "title": "Threads",
      "width": 2,
      "metrics": [
        {
          "name": "activeThreads"
        },
        {
          "name": "totalThreads"
        }
      ]
    },
    {
      "dataSource": "jvm-metrics",
      "title": "Heap",
      "width": 1,
      "yAxis": [
        {
          "minInterval": 1048576,
          "interval": 1073741824,
          "unit": "binary_byte"
        }
      ],
      "metrics": [
        {
          "name": "heapMax"
        },
        {
          "name": "heapUsed"
        },
        {
          "name": "heapCommitted"
        }
      ]
    },
    {
      "dataSource": "jvm-metrics",
      "title": "Non-Heap",
      "width": 1,
      "yAxis": [
        {
          "minInterval": 1048576,
          "interval": 1073741824,
          "unit": "binary_byte"
        }
      ],
      "metrics": [
        {
          "name": "nonHeapMax"
        },
        {
          "name": "nonHeapUsed"
        },
        {
          "name": "nonHeapCommitted"
        }
      ]
    },
    {
      "dataSource": "jvm-metrics",
      "title": "Metaspace",
      "width": 1,
      "yAxis": [
        {
          "minInterval": 1048576,
          "interval": 1073741824,
          "unit": "binary_byte"
        }
      ],
      "metrics": [
        {
          "name": "metaspaceUsed"
        },
        {
          "name": "metaspaceCommitted",
          "selected": false
        },
        {
          "name": "metaspaceMax",
          "selected": false
        }
      ]
    },
    {
      "dataSource": "jvm-metrics",
      "title": "Direct Memory",
      "width": 1,
      "yAxis": [
        {
          "minInterval": 1048576,
          "interval": 1073741824,
          "unit": "binary_byte"
        }
      ],
      "metrics": [
        {
          "name": "directUsed"
        },
        {
          "name": "directMax"
        }
      ]
    },
    {
      "dataSource": "jvm-gc-metrics",
      "title": "Young GC",
      "width": 4,
      "yAxis": [
        {
        },
        {
          "unit": "millisecond"
        }
      ],
      "dimensions": {
        "generation": {
          "dimension": "generation",
          "matcher": {
            "type": "equal",
            "pattern": "new"
          }
        }
      },
      "metrics": [
        {
          "name": "gcCount"
        },
        {
          "name": "avgGcTime",
          "yAxis": 1
        }
      ]
    },
    {
      "dataSource": "jvm-gc-metrics",
      "title": "Old GC",
      "width": 4,
      "yAxis": [
        {
        },
        {
          "unit": "millisecond"
        }
      ],
      "dimensions": {
        "generation": {
          "dimension": "generation",
          "matcher": {
            "type": "equal",
            "pattern": "old"
          }
        }
      },
      "metrics": [
        {
          "name": "gcCount"
        },
        {
          "name": "avgGcTime",
          "yAxis": 1
        }
      ]
    }
  ]
}
