{
  "title": "HTTP Outgoing",
  "charts": [
    {
      "dataSource": "http-outgoing-metrics",
      "title": "QPS",
      "width": 4,
      "metrics": [
        {
          "name": "qps"
        }
      ],
      "details": {
        "groupBy": [
          "targetHostPort",
          "path",
          "statusCode"
        ],
        "metrics": [
          "qps"
        ]
      }
    },
    {
      "dataSource": "http-outgoing-metrics",
      "title": "Response Time",
      "width": 4,
      "yAxis": [
        {
          "format": "millisecond"
        }
      ],
      "metrics": [
        {
          "name": "minResponseTime"
        },
        {
          "name": "avgResponseTime"
        },
        {
          "name": "maxResponseTime"
        }
      ],
      "details": {
        "groupBy": [
          "targetHostPort",
          "path",
          "statusCode"
        ],
        "metrics": [
          "minResponseTime",
          "avgResponseTime",
          "maxResponseTime"
        ]
      }
    },
    {
      "dataSource": "http-outgoing-metrics",
      "title": "Error",
      "width": 4,
      "metrics": [
        {
          "name": "count4xx"
        },
        {
          "name": "count5xx"
        },
        {
          "name": "countException"
        }
      ],
      "details": {
        "groupBy": [
          "targetHostPort",
          "path",
          "statusCode"
        ],
        "metrics": [
          "requestCount"
        ],
        "filters": [
          {
            "dimension": "statusCode",
            "matcher": {
              "type": "notEqual",
              "pattern": "200"
            }
          }
        ]
      }
    },
    {
      "dataSource": "http-outgoing-metrics",
      "title": "IO",
      "width": 4,
      "yAxis": [
        {
          "format": "byte_rate"
        },
        {
          "format": "binary_byte"
        }
      ],
      "metrics": [
        {
          "name": "requestByteRate"
        },
        {
          "name": "responseByteRate"
        },
        {
          "name": "requestBytes",
          "yAxis": 1
        },
        {
          "name": "responseBytes",
          "yAxis": 1
        }
      ],
      "details": {
        "groupBy": [
          "targetHostPort",
          "path",
          "statusCode"
        ],
        "metrics": [
          "requestByteRate",
          "responseByteRate"
        ]
      }
    }
  ]
}
