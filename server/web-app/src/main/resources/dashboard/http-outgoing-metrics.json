{
  "title": "HTTP Outgoing",
  "charts": [
    {
      "dataSource": "http-outgoing-metrics",
      "title": "QPS",
      "width": 4,
      "type": "line",
      "columns": [
        "qps"
      ],
      "query": {
        "dataSource": "http-outgoing-metrics",
        "fields": [
          "qps"
        ]
      },
      "details": {
        "type": "groupBy",
        "columns": [
          "targetHostPort",
          "path",
          "statusCode",
          "qps"
        ],
        "query": {
          "type": "timeseries",
          "dataSource": "http-outgoing-metrics",
          "fields": [
            "targetHostPort",
            "path",
            "statusCode",
            "qps"
          ],
          "orderBy": {
            "name": "qps",
            "order": "desc"
          }
        }
      }
    },
    {
      "dataSource": "http-outgoing-metrics",
      "title": "Response Time",
      "width": 4,
      "type": "line",
      "columns": [
        {"name": "minResponseTime", "format": "nanosecond"},
        {"name": "avgResponseTime", "format": "millisecond"},
        {"name": "maxResponseTime", "format": "nanosecond"}
      ],
      "query": {
        "type": "timeseries",
        "dataSource": "http-outgoing-metrics",
        "fields": [
          "minResponseTime",
          "avgResponseTime",
          "maxResponseTime"
        ]
      },
      "details": {
        "columns": [
          "targetHostPort",
          "path",
          "statusCode",
          "requestCount",
          {"name": "minResponseTime", "title": "min", "format": "nanosecond"},
          {"name": "avgResponseTime", "title": "avg", "format": "millisecond"},
          {"name": "maxResponseTime", "title": "max", "format": "nanosecond"}
        ],
        "query": {
          "type": "groupBy",
          "fields": [
            "targetHostPort",
            "path",
            "statusCode",
            "requestCount",
            "minResponseTime",
            "avgResponseTime",
            "maxResponseTime"
          ],
          "orderBy": {
            "name": "avgResponseTime",
            "order": "desc"
          }
        }
      }
    },
    {
      "dataSource": "http-outgoing-metrics",
      "title": "Errors",
      "width": 4,
      "type": "line",
      "columns": [
        "count4xx",
        "count5xx",
        "countException"
      ],
      "query": {
        "dataSource": "http-outgoing-metrics",
        "type": "timeseries",
        "fields": [
          "count4xx",
          "count5xx",
          "countException"
        ]
      },
      "details": {
        "columns": [
          "targetHostPort",
          "path",
          "statusCode",
          "requestCount"
        ],
        "query": {
          "type": "groupBy",
          "fields": [
            "targetHostPort",
            "path",
            "statusCode",
            "requestCount"
          ],
          "filters": [
            {
              "dimension": "statusCode",
              "matcher": {
                "type": "gt",
                "value": "200"
              }
            }
          ]
        }
      }
    },
    {
      "dataSource": "http-outgoing-metrics",
      "title": "IO",
      "width": 4,
      "type": "line",
      "yAxis": [
        {
          "format": "byte_rate"
        },
        {
          "format": "binary_byte"
        }
      ],
      "columns": [
        "requestByteRate",
        "responseByteRate",
        {
          "name": "requestBytes",
          "yAxis": 1
        },
        {
          "name": "responseBytes",
          "yAxis": 1
        }
      ],
      "query": {
        "dataSource": "http-outgoing-metrics",
        "type": "timeseries",
        "fields": [
          "requestByteRate",
          "responseByteRate",
          "requestBytes",
          "responseBytes"
        ]
      },
      "details": {
        "columns": [
          "targetHostPort",
          "path",
          "statusCode",
          "requestByteRate",
          "responseByteRate"
        ],
        "query": {
          "type": "groupBy",
          "fields": [
            "targetHostPort",
            "path",
            "statusCode",
            "requestByteRate",
            "responseByteRate"
          ]
        }
      }
    }
  ]
}
