{
  "name": "http-outgoing-metrics",
  "title": "HTTP Outgoing",
  "folder": "metrics",
  "filter": {
    "selectors": [
      {
        "type": "datasource",
        "name": "http-outgoing-metrics",
        "fields": [
          {"name": "appName", "width": 200, "allowClear": false},
          "instanceName",
          "targetHostPort",
          "method",
          "path",
          "statusCode"
        ]
      }
    ],
    "showFilterInput": true
  },
  "charts": [
    {
      "title": "QPS",
      "width": 4,
      "type": "line",
      "columns": [
        "qps",
        {"name": "requestCount", "yAxis": 1}
      ],
      "query": {
        "dataSource": "http-outgoing-metrics",
        "fields": [
          "qps",
          "requestCount"
        ]
      },
      "details": {
        "type": "groupBy",
        "columns": [
          "targetHostPort",
          "method",
          "path",
          "statusCode",
          "qps",
          "requestCount"
        ],
        "query": {
          "type": "timeseries",
          "dataSource": "http-outgoing-metrics",
          "fields": [
            "targetHostPort",
            "method",
            "path",
            "statusCode",
            "qps",
            "requestCount"
          ],
          "orderBy": {
            "name": "qps",
            "order": "desc"
          },
          "limit": 15
        },
        "tracing": {
          "mappings": {
            "targetHostPort": "tags.net.peer",
            "method": "tags.http.method",
            "path": "url",
            "statusCode": "status"
          },
          "filter": "name= 'httpclient' AND kind = 'CLIENT'"
        }
      }
    },
    {
      "title": "Response Time",
      "width": 4,
      "type": "line",
      "columns": [
        {"name": "minResponseTime", "format": "nanosecond"},
        {"name": "avgResponseTime", "format": "nanosecond"},
        {"name": "maxResponseTime", "format": "nanosecond"}
      ],
      "query": {
        "type": "timeseries",
        "dataSource": "http-outgoing-metrics",
        "fields": [
          "minResponseTime",
          "avgResponseTime",
          "maxResponseTime"
        ]
      },
      "details": {
        "columns": [
          "targetHostPort",
          "method",
          "path",
          "statusCode",
          "requestCount",
          {"name": "minResponseTime", "title": "min", "format": "nanosecond"},
          {"name": "avgResponseTime", "title": "avg", "format": "nanosecond"},
          {"name": "maxResponseTime", "title": "max", "format": "nanosecond"}
        ],
        "query": {
          "type": "groupBy",
          "fields": [
            "targetHostPort",
            "method",
            "path",
            "statusCode",
            "requestCount",
            "minResponseTime",
            "avgResponseTime",
            "maxResponseTime"
          ],
          "orderBy": {
            "name": "avgResponseTime",
            "order": "desc"
          },
          "limit": 15
        },
        "tracing": {
          "mappings": {
            "targetHostPort": "tags.net.peer",
            "method": "tags.http.method",
            "path": "url",
            "statusCode": "status"
          },
          "filter": "name= 'httpclient' AND kind = 'CLIENT'"
        }
      }
    },
    {
      "title": "Errors",
      "width": 4,
      "type": "line",
      "columns": [
        "count4xx",
        "count5xx",
        "countException"
      ],
      "query": {
        "dataSource": "http-outgoing-metrics",
        "type": "timeseries",
        "fields": [
          "count4xx",
          "count5xx",
          "countException"
        ]
      },
      "details": {
        "columns": [
          "targetHostPort",
          "method",
          "path",
          "statusCode",
          "requestCount"
        ],
        "query": {
          "type": "groupBy",
          "fields": [
            "targetHostPort",
            "method",
            "path",
            "statusCode",
            "requestCount"
          ],
          "filter": "statusCode >= '400'",
          "orderBy": {
            "name": "requestCount",
            "order": "desc"
          },
          "limit": 15
        },
        "tracing": {
          "mappings": {
            "targetHostPort": "tags.net.peer",
            "method": "tags.http.method",
            "path": "url",
            "statusCode": "status"
          },
          "filter": "name= 'httpclient' AND kind = 'CLIENT'"
        }
      }
    },
    {
      "title": "IO",
      "width": 4,
      "type": "line",
      "yAxis": [
        {
          "format": "byte_rate"
        },
        {
          "format": "binary_byte"
        }
      ],
      "columns": [
        "requestByteRate",
        "responseByteRate",
        {
          "name": "requestBytes",
          "yAxis": 1
        },
        {
          "name": "responseBytes",
          "yAxis": 1
        }
      ],
      "query": {
        "dataSource": "http-outgoing-metrics",
        "type": "timeseries",
        "fields": [
          "requestByteRate",
          "responseByteRate",
          "requestBytes",
          "responseBytes"
        ]
      },
      "details": {
        "columns": [
          "targetHostPort",
          "method",
          "path",
          "requestCount",
          "statusCode",
          {"name":"requestByteRate","format": "byte_rate"},
          {"name":"responseByteRate","format": "byte_rate"}
        ],
        "query": {
          "type": "groupBy",
          "fields": [
            "targetHostPort",
            "method",
            "path",
            "requestCount",
            "statusCode",
            "requestByteRate",
            "responseByteRate"
          ],
          "orderBy": {
            "name": "requestByteRate",
            "order": "desc"
          },
          "limit": 15
        }
      }
    }
  ]
}
