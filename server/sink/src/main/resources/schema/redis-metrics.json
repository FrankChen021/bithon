{
  "displayText": "Redis",
  "name": "redis-metrics",
  "timestampSpec": {
    "column": "timestamp",
    "format": "auto"
  },
  "dimensionsSpec": [
    {
      "name": "appName",
      "displayText": "Application",
      "required": true
    },
    {
      "name": "instanceName",
      "displayText": "Instance",
      "required": true
    },
    {
      "name": "uri",
      "displayText": "Redis Server",
      "required": true
    },
    {
      "name": "command",
      "displayText": "Redis Command",
      "required": true
    }
  ],
  "metricsSpec": [
    {
      "type": "longSum",
      "name": "exceptionCount",
      "displayText": "Exception Command",
      "unit": ""
    },
    {
      "type": "longSum",
      "name": "totalCount",
      "displayText": "Total Commands",
      "unit": ""
    },
    {
      "type": "longMin",
      "name": "minResponseTime",
      "displayText": "Min Response Time",
      "unit": "nanosecond"
    },
    {
      "type": "longSum",
      "name": "responseTime",
      "displayText": "Response Time",
      "unit": "nanosecond"
    },
    {
      "type": "longMax",
      "name": "maxResponseTime",
      "displayText": "Max Response Time",
      "unit": "nanosecond"
    },
    {
      "type": "post",
      "name": "avgRequestTime",
      "expression": "requestTime/totalCount",
      "displayText": "AVG Request Time",
      "unit": "nanosecond",
      "valueType": "double"
    },
    {
      "type": "post",
      "name": "qps",
      "expression": "totalCount/{interval}",
      "displayText": "QPS",
      "unit": "",
      "valueType": "double"
    },
    {
      "type": "post",
      "name": "avgResponseTime",
      "expression": "responseTime/totalCount",
      "displayText": "AVG Response Time",
      "unit": "nanosecond",
      "valueType": "double"
    },
    {
      "type": "longSum",
      "name": "requestBytes",
      "displayText": "Request Bytes",
      "unit": "byte"
    },
    {
      "type": "longSum",
      "name": "responseBytes",
      "displayText": "Response Bytes",
      "unit": "byte"
    },
    {
      "type": "post",
      "name": "requestByteRate",
      "expression": "requestBytes/{interval}",
      "displayText": "Avg Request Byte Rate",
      "unit": "Bps",
      "valueType": "double"
    },
    {
      "type": "post",
      "name": "responseByteRate",
      "expression": "responseBytes/{interval}",
      "displayText": "Avg Response Byte Rate",
      "unit": "Bps",
      "valueType": "double"
    }
  ],
  "inputSourceSpec": {
    "type": "metric",
    "transformSpec": {
      "prefilter": {
        "type": "expression",
        "expression": "totalCount > 0"
      }
    }
  }
}



