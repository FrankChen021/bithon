{
  "filter": {
    "selectors": [
      {
        "type": "datasource",
        "name": "jvm-metrics",
        "fields": [
          {
            "name": "appName",
            "width": 200
          },
          {
            "name": "instanceName",
            "width": 300
          }
        ]
      }
    ]
  },
  "charts": [
    {
      "type": "stat",
      "width": 1,
      "titleOption": {
        "title": "Avg Cpu Usage",
        "link": "/dashboard/jvm-metrics?appName={appName}&instanceName={instanceName}",
        "align": "left"
      },
      "minimapOption": {
        "type": "area"
      },
      "valueOption": {
        "reducer": "avg",
        "align": "center"
      },
      "query": {
        "dataSource": "jvm-metrics",
        "type": "timeseries",
        "fields": [
          {
            "name": "avgCpuUsage",
            "expression": "avg(processCpuLoad)"
          }
        ],
        "precondition": {
          "filters": [
            "appName"
          ]
        }
      }
    },
    {
      "type": "stat",
      "width": 1,
      "titleOption": {
        "title": "Instances",
        "link": "/dashboard/jvm-metrics?appName={appName}&instanceName={instanceName}",
        "align": "left"
      },
      "minimapOption": {
        "type": "area"
      },
      "valueOption": {
        "reducer": "last",
        "align": "center"
      },
      "query": {
        "dataSource": "jvm-metrics",
        "type": "timeseries",
        "fields": [
          {
            "name": "instanceCount",
            "expression": "cardinality(instanceName)"
          }
        ],
        "precondition": {
          "filters": []
        }
      }
    },
    {
      "type": "stat",
      "width": 1,
      "titleOption": {
        "title": "Incoming HTTP Requests",
        "link": "/dashboard/http-incoming-metrics?appName={appName}&instanceName={instanceName}",
        "align": "left"
      },
      "minimapOption": {
        "type": "area"
      },
      "valueOption": {
        "reducer": "sum",
        "align": "center"
      },
      "comparisonOption": {
        "offset": "-1d"
      },
      "query": {
        "dataSource": "http-incoming-metrics",
        "type": "timeseries",
        "fields": [
          {
            "name": "totalCount",
            "expression": "sum(totalCount)"
          }
        ],
        "precondition": {
          "filters": []
        }
