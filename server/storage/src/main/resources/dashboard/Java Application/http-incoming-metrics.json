{
  "id": "http-incoming-metrics",
  "title": "HTTP Incoming",
  "filter": {
    "selectors": [
      {
        "type": "datasource",
        "name": "http-incoming-metrics",
        "fields": [
          {
            "name": "appName",
            "width": 200,
            "allowClear": false
          },
          "instanceName",
          "method",
          "uri",
          "statusCode"
        ]
      }
    ],
    "showFilterInput": true
  },
  "charts": [
    {
      "title": "Top 10 Incoming HTTP Requests",
      "width": 4,
      "type": "table",
      "columns": [
        "uri",
        {
          "name": "qps",
          "sortable": true
        },
        {
          "name": "totalCount",
          "format": "compact_number",
          "sortable": "true"
        },
        {
          "name": "errorCount",
          "format": "compact_number",
          "sortable": "true"
        },
        {
          "name": "minResponseTime",
          "format": "nanosecond",
          "sortable": "true"
        },
        {
          "name": "avgResponseTime",
          "format": "nanosecond",
          "sortable": "true"
        },
        {
          "name": "maxResponseTime",
          "format": "nanosecond",
          "sortable": "true"
        }
      ],
      "query": {
        "dataSource": "http-incoming-metrics",
        "type": "groupBy",
        "fields": [
          {"name": "qps",             "expression": "round(sum(totalCount)/{interval}, 2)"},
          {"name": "totalCount",      "expression": "sum(totalCount)"},
          {"name": "errorCount",      "expression": "sum(errorCount)"},
          {"name": "minResponseTime", "expression": "min(minResponseTime)"},
          {"name": "avgResponseTime", "expression": "round(sum(responseTime) * 1.0 /sum(totalCount),2)"},
          {"name": "maxResponseTime", "expression": "max(maxResponseTime)"}
        ],
        "limit": 10,
        "groupBy": [
          "appName",
          "uri"
        ],
        "orderBy": {
          "name": "totalCount",
          "order": "desc"
        },
        "precondition": {
          "filters": [
            "appName"
          ]
        }
      }
    },
    {
      "title": "QPS",
      "width": 4,
      "type": "line",
      "columns": [
        "qps",
        "okQPS",
        {
          "name": "totalCount",
          "yAxis": 1
        }
      ],
      "query": {
        "type": "timeseries",
        "dataSource": "http-incoming-metrics",
        "fields": [
          {"name": "qps",         "expression": "round(sum(totalCount)/{interval}, 2)"},
          {"name": "okQPS",       "expression": "round(sum(okCount)/{interval}, 2)"},
          {"name": "totalCount",  "expression": "sum(totalCount)"}
        ],
        "details": {
          "columns": [
            "method",
            "uri",
            "statusCode",
            "totalCount",
            "qps"
          ],
          "query": {
            "dataSource": "http-incoming-metrics",
            "fields": [
              {"name": "qps",         "expression": "round(sum(totalCount)/{interval}, 2)"},
              {"name": "totalCount",  "expression": "sum(totalCount)"}
            ],
            "groupBy": [
              "method",
              "uri",
              "statusCode"
            ],
            "orderBy": {
              "name": "totalCount",
              "order": "desc"
            },
            "limit": 15
          },
          "tracing": {
            "mappings": {
              "uri": "url",
              "statusCode": "status"
            },
            "filter": "kind = 'SERVER'"
          }
        }
      }
    },
    {
      "title": "Response Time",
      "width": 4,
      "type": "line",
      "yAxis": [
        {
          "format": "nanosecond"
        }
      ],
      "columns": [
        "minResponseTime",
        "avgResponseTime",
        "maxResponseTime"
      ],
      "query": {
        "dataSource": "http-incoming-metrics",
        "type": "timeseries",
        "fields": [
          {"name": "minResponseTime", "expression": "min(minResponseTime)"},
          {"name": "avgResponseTime", "expression": "round(sum(responseTime) * 1.0 /sum(totalCount),2)"},
          {"name": "maxResponseTime", "expression": "max(maxResponseTime)"}
        ]
      },
      "details": {
        "columns": [
          "method",
          "uri",
          "statusCode",
          "totalCount",
          {
            "name": "minResponseTime",
            "format": "nanosecond"
          },
          {
            "name": "avgResponseTime",
            "format": "nanosecond"
          },
          {
            "name": "maxResponseTime",
            "format": "nanosecond"
          }
        ],
        "query": {
          "dataSource": "http-incoming-metrics",
          "type": "groupBy",
          "fields": [
            {"name": "totalCount",      "expression": "sum(totalCount)"},
            {"name": "minResponseTime", "expression": "min(minResponseTime)"},
            {"name": "avgResponseTime", "expression": "round(sum(responseTime) * 1.0 /sum(totalCount),2)"},
            {"name": "maxResponseTime", "expression": "max(maxResponseTime)"}
          ],
          "groupBy": [
            "method",
            "uri",
            "statusCode"
          ],
          "orderBy": {
            "name": "avgResponseTime",
            "order": "desc"
          },
          "limit": 15
        },
        "tracing": {
          "mappings": {
            "uri": "url",
            "statusCode": "status"
          },
          "filter": "kind = 'SERVER'"
        }
      }
    },
    {
      "title": "Errors",
      "width": 4,
      "type": "line",
      "columns": [
        "count4xx",
        "count5xx",
        "flowedCount",
        "degradedCount"
      ],
      "query": {
        "dataSource": "http-incoming-metrics",
        "type": "timeseries",
        "fields": [
          {"name": "count4xx",     "expression": "sum(count4xx)"},
          {"name": "count5xx",     "expression": "sum(count5xx)"},
          {"name": "flowedCount",  "expression": "sum(flowedCount)"},
          {"name": "degradedCount", "expression": "sum(degradedCount)"}
        ]
      },
      "details": {
        "dataSource": "http-incoming-metrics",
        "columns": [
          "method",
          "uri",
          "statusCode",
          "totalCount"
        ],
        "query": {
          "type": "groupBy",
          "fields": [
            {"name" : "totalCount", "expression": "sum(totalCount)"}
          ],
          "groupBy": [
            "method",
            "uri",
            "statusCode"
          ],
          "orderBy": {
            "name": "totalCount",
            "order": "desc"
          },
          "filter": "statusCode >= '400'",
          "limit": 15
        },
        "tracing": {
          "mappings": {
            "uri": "url",
            "statusCode": "status"
          },
          "filter": "kind = 'SERVER'"
        }
      }
    },
    {
      "title": "IO",
      "width": 4,
      "type": "line",
      "yAxis": [
        {
          "format": "byte_rate"
        },
        {
          "format": "binary_byte"
        }
      ],
      "columns": [
        "requestByteRate",
        "responseByteRate",
        {
          "name": "requestBytes",
          "yAxis": 1
        },
        {
          "name": "responseBytes",
          "yAxis": 1
        }
      ],
      "query": {
        "dataSource": "http-incoming-metrics",
        "type": "timeseries",
        "fields": [
          {"name": "requestByteRate",  "expression": "round(sum(requestBytes)/{interval}, 2)"},
          {"name": "responseByteRate", "expression": "round(sum(responseBytes)/{interval}, 2)"},
          {"name": "requestBytes",     "expression": "sum(requestBytes)"},
          {"name": "responseBytes"  ,  "expression": "sum(responseBytes)"}
        ]
      },
      "details": {
        "columns": [
          "method",
          "uri",
          "statusCode",
          "totalCount",
          {
            "name": "requestByteRate",
            "format": "byte_rate"
          },
          {
            "name": "responseByteRate",
            "format": "byte_rate"
          }
        ],
        "query": {
          "type": "groupBy",
          "fields": [
            {"name": "totalCount",       "expression": "sum(totalCount)"},
            {"name": "requestByteRate",  "expression": "round(sum(requestBytes)/{interval}, 2)"},
            {"name": "responseByteRate", "expression": "round(sum(responseBytes)/{interval}, 2)"}
          ],
          "groupBy": [
            "method",
            "uri",
            "statusCode"
          ],
          "orderBy": {
            "name": "requestByteRate",
            "order": "desc"
          },
          "limit": 15
        }
      }
    }
  ]
}
