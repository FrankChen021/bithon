{
  "id": "redis-metrics",
  "title": "Redis",
  "filter": {
    "selectors": [
      {
        "type": "datasource",
        "name": "redis-metrics",
        "fields": [{ "name": "appName", "width": 200, "allowClear": false }, "instanceName", "uri", "command"]
      }
    ],
    "showFilterInput": true
  },
  "charts": [
    {
      "title": "Commands",
      "width": 4,
      "type": "line",
      "columns": [
        "qps",
        {
          "name": "totalCount",
          "yAxis": 1
        }
      ],
      "query": {
        "type": "timeseries",
        "dataSource": "redis-metrics",
        "fields": [
          {
            "name": "qps",
            "expression": "round(sum(totalCount)/{interval}, 2)"
          },
          {
            "name": "totalCount",
            "expression": "sum(totalCount)"
          }
        ]
      },
      "details": {
        "columns": ["uri", "command", "qps", "totalCount"],
        "query": {
          "type": "groupBy",
          "fields": [
            {
              "name": "qps",
              "expression": "round(sum(totalCount)/{interval}, 2)"
            },
            {
              "name": "totalCount",
              "expression": "sum(totalCount)"
            }
          ],
          "groupBy": ["uri", "command"],
          "orderBy": {
            "name": "totalCount",
            "order": "desc"
          }
        },
        "tracing": {
          "mappings": {
            "uri": "tags['net.peer']",
            "command": "tags['db.operation.name']"
          },
          "filter": "kind = 'CLIENT' AND tags['db.system']='redis'"
        }
      }
    },
    {
      "title": "Time",
      "width": 4,
      "type": "line",
      "yAxis": [
        {
          "format": "nanoFormatter"
        }
      ],
      "columns": ["minResponseTime", "avgResponseTime", "maxResponseTime"],
      "query": {
        "type": "timeseries",
        "dataSource": "redis-metrics",
        "fields": [
          {
            "name": "minResponseTime",
            "expression": "min(minResponseTime)"
          },
          {
            "name": "avgResponseTime",
            "expression": "round(sum(responseTime) * 1.0 /sum(totalCount),2)"
          },
          {
            "name": "maxResponseTime",
            "expression": "max(maxResponseTime)"
          }
        ]
      }
    },
    {
      "title": "IO",
      "width": 4,
      "type": "line",
      "yAxis": [
        {
          "format": "byte_rate"
        },
        {
          "format": "binary_byte"
        }
      ],
      "columns": [
        {
          "name": "requestByteRate",
          "format": "byte_rate",
          "yAxis": 0
        },
        {
          "name": "responseByteRate",
          "format": "byte_rate",
          "yAxis": 0
        },
        {
          "name": "requestBytes",
          "format": "binary_byte",
          "yAxis": 1
        },
        {
          "name": "responseBytes",
          "format": "binary_byte",
          "yAxis": 1
        }
      ],
      "query": {
        "type": "timeseries",
        "dataSource": "redis-metrics",
        "fields": [
          {
            "name": "requestByteRate",
            "expression": "round(sum(requestBytes)/{interval}, 2)"
          },
          {
            "name": "responseByteRate",
            "expression": "round(sum(responseBytes)/{interval}, 2)"
          },
          {
            "name": "requestBytes",
            "expression": "sum(requestBytes)"
          },
          {
            "name": "responseBytes",
            "expression": "sum(responseBytes)"
          }
        ]
      }
    }
  ]
}
