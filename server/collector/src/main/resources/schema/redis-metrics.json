{
  "displayText": "Redis",
  "name": "redis-metrics",
  "timestampSpec": {
    "column": "timestamp",
    "format": "auto"
  },

  "dimensionsSpec": [
    {
      "name": "appName",
      "displayText": "Application Name",
      "required": true
    },
    {
      "name": "instanceName",
      "displayText": "Instance Name",
      "required": true
    },
    {
      "name": "uri",
      "displayText": "Redis Server",
      "required": true
    },
    {
      "name": "command",
      "displayText": "Redis Command",
      "required": true
    }
  ],
  "metricsSpec": [
    {
      "type": "longSum",
