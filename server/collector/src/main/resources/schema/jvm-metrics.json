{
  "displayText": "JVM",
  "name": "jvm-metrics",
  "timestampSpec": {
    "column": "timestamp",
    "format": "auto"
  },
  "dimensionsSpec": [
    {
      "name": "appName",
      "displayText": "Application",
      "required": true
    },
    {
      "name": "instanceName",
      "displayText": "Instance",
      "required": true
    }
  ],
  "metricsSpec": [
    {
      "type": "longLast",
      "name": "processors",
      "displayText": "Processors",
      "unit": "count"
    },
    {
      "type": "longLast",
      "name": "processCpuTime",
      "displayText": "Processors",
      "unit": "nanosecond"
    },
    {
      "type": "doubleLast",
      "name": "processCpuLoad",
      "displayText": "CPU Load",
      "unit": "%"
    },
    {
      "type": "longLast",
      "name": "instanceStartTime",
      "displayText": "Start Time",
      "unit": "Milli Second"
    },
    {
      "type": "longLast",
      "name": "instanceUpTime",
      "displayText": "Running Time",
      "unit": "millisecond"
    },
    {
      "type": "longLast",
      "name": "heap",
      "displayText": "Heap(-Xmx)",
      "unit": "byte"
    },
    {
      "type": "longLast",
      "name": "heapInit",
      "displayText": "Init Heap Size(-Xms)",
      "unit": "byte"
    },
    {
      "type": "longLast",
      "name": "heapUsed",
      "displayText": "Heap Used",
      "unit": "byte"
    },
    {
      "type": "longLast",
      "name": "heapCommitted",
      "displayText": "Available Heap",
      "unit": "byte"
    },
    {
      "type": "longLast",
      "name": "peakThreads",
      "displayText": "Peek Threads",
      "unit": "count"
    },
    {
      "type": "longLast",
      "name": "daemonThreads",
      "displayText": "Daemon Threads",
      "unit": "count"
    },
    {
      "type": "longLast",
      "name": "totalThreads",
      "displayText": "Total Threads",
      "unit": "count"
    },
    {
      "type": "longLast",
      "name": "activeThreads",
      "displayText": "Active Threads",
      "unit": "count"
    }
  ]
}



