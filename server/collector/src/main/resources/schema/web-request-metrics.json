{
  "displayText": "Web Request",
  "name": "web-request-metrics",
  "timestampSpec": {
    "column": "timestamp",
    "format": "auto"
  },
  "dimensionsSpec": [
    {
      "name": "appName",
      "displayText": "Application Name",
      "required": true
    },
    {
      "name": "instanceName",
      "displayText": "Instance Name",
      "required": true
    },
    {
      "name": "srcApplication",
      "displayText": "Source Application",
      "required": true
    },
    {
      "name": "uri",
      "displayText": "Request URI",
      "required": true
    }
  ],
  "metricsSpec": [
    {
      "type": "longSum",
      "name": "costTime",
      "displayText": "Accumulative cost Time",
      "unit": "Nano Second",
      "visible": false
    },
    {
      "type": "longMin",
      "name": "minCostTime",
      "displayText": "min cost time",
      "unit": "Nano Second"
    },
    {
      "type": "longMax",
      "name": "maxCostTime",
      "displayText": "Max cost time",
      "unit": "Nano Second"
    },
    {
      "type": "longSum",
      "name": "interval",
      "displayText": "Period of metric",
      "unit": "second",
      "visible": false
    },
    {
      "type": "longSum",
      "name": "requestCount",
      "displayText": "count of requests",
      "unit": ""
    },
    {
      "type": "longSum",
      "name": "errorCount",
      "displayText": "Active Threads",
      "unit": ""
    },
    {
      "type": "longSum",
      "name": "count4xx",
      "displayText": "Max Threads",
      "unit": ""
    },
    {
      "type": "longSum",
      "name": "count5xx",
      "displayText": "Max Threads",
      "unit": ""
    },
    {
      "type": "longSum",
      "name": "requestByteSize",
      "displayText": "Bytes of Request",
      "unit": "Byte"
    },
    {
      "type": "longSum",
      "name": "responseByteSize",
      "displayText": "Bytes of Responses",
      "unit": "Byte"
    },
    {
      "type": "postAggregator",
      "name": "avgCostTime",
      "expression": "costTime/1000.0/1000/requestCount",
      "displayText": "Avg cost time",
      "unit": "Milli",
      "valueType": "double"
    },
    {
      "type": "postAggregator",
      "name": "qps",
      "expression": "requestCount/interval",
      "displayText": "QPS",
      "unit": "",
      "valueType": "double"
    }
  ]
}



